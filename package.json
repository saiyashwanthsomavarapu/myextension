{
  "name": "HIGWAY",
  "displayName": "RENudgeExtension",
  "description": "RE Nudge Extension",
  "version": "0.0.1",
  "publisher": "TheHartford",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "higway-sidebar-view",
          "title": "HIGWAY",
          "icon": "media/productivity-expert-icon.svg"
        }
      ]
    },
    "views": {
      "higway-sidebar-view": [
        {
          "type": "webview",
          "id": "higway-sidebar",
          "name": "HIGWAY",
          "icon": "media/heproductivity-expert-iconart.svg",
          "contextualTitle": "HIGWAY"
        }
      ]
    },
    "commands": [
      {
        "command": "HIGWAY.getByMax",
        "title": "Get By Max Value",
        "category": "TODO"
      },
      {
        "command": "HIGWAY.run",
        "title": "Todo",
        "category": "TODO"
      },
      {
        "command": "HIGWAY.helloWorld",
        "title": "Hello World",
        "category": "HIGWAY"
      },
      {
        "title": "First Command",
        "command": "HIGWAY.FirstCommand",
        "category": "HIGWAY"
      },
      {
        "title": "Scan Document",
        "command": "HIGWAY.ScanDocument",
        "category": "HIGWAY"
      },
      {
        "title": "Prompt Test",
        "command": "HIGWAY.PromptTest",
        "category": "HIGWAY"
      },
      {
        "title": "Terminal Test",
        "command": "HIGWAY.TerminalTest",
        "category": "HIGWAY"
      }
    ]
  },
  "scripts": {
    "webview:build": "npm run compile && npm build",
    "vite:build": "vite build",
    "vscode:prepublish": "npm run compile",
    "package": "vsce package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.83.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.3.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.54.17",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
